TOPDIR=../..
OS_ARCH='Linux'

DIR_INCLUDES = -I$(TOPDIR)/src/DOMnocorba -I. 
DIR_CXXFLAGS = -rdynamic
DIR_CPPFLAGS = -D_MIT_POSIX_THREADS -D_REENTRANT
DIR_LDFLAGS  = -lpthread -lstdc++ -ldl

CXXSRCS = domString.cc domNode_impl.cc domText_impl.cc \
          domAttrib_impl.cc testIt.cc \
          domElement_impl.cc domNodeIterator_impl.cc \
          domAttributeList_impl.cc domDocument_impl.cc

CXXOBJS = domString.o domNode_impl.o domText_impl.o \
          domAttrib_impl.o testIt.o \
          domElement_impl.o domNodeIterator_impl.o \
          domAttributeList_impl.o domDocument_impl.o

EX_TARGET = testText

EX_OBJS_testText = $(CXXSRCS:.cc=.o)
EX_CFLAGS_testText = -rdynamic


include $(TOPDIR)/config/rules.mk

