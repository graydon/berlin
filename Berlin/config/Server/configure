char*
my_strdup (char *str)
{
  char *new_str;
  
  if (str)
    {
      new_str = (char *)malloc ((strlen (str) + 1) * sizeof(char));
      strcpy (new_str, str);
    }
  else
    new_str = NULL;
  
  return new_str;
}

int main (int argc, char *argv[])
{
  int major, minor, patch;
  char *tmp_version;

  /* This hangs on some systems (?)
  system ("touch conf.Praguetest");
  */
  { FILE *fp = fopen("conf.Praguetest", "a"); if ( fp ) fclose(fp); }

  /* HP/UX 9 (%@#!) writes to sscanf strings */
  tmp_version = my_strdup("$min_Prague_version");
  if (sscanf(tmp_version, "%d.%d.%d", &major, &minor, &patch) != 3) {
     printf("%s, bad version string\n", "$min_Prague_version");
     exit(1);
   }

   if (($Prague_major_version > major) ||
      (($Prague_major_version == major) && ($Prague_minor_version > minor)) ||
      (($Prague_major_version == major) && ($Prague_minor_version == minor) && ($Prague_patch_level >= patch)))
    {
      return 0;
    }
  else
    {
      printf("\n*** 'Prague-config --version' returned %d.%d.%d, but the minimum version\n", $Prague_major_version, $Prague_minor_version, $Prague_patch_level);
      printf("*** of Prague required is %d.%d.%d. If Prague-config is correct, then it is\n", major, minor, micro);
      printf("*** best to upgrade to the required version.\n");
      printf("*** If Prague-config was wrong, set the environment variable Prague_CONFIG\n");
      printf("*** to point to the correct copy of Prague-config, and remove the file\n");
      printf("*** config.cache before re-running configure\n");
      return 1;
    }
}


